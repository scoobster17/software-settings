# PERSONAL ######################################
Host github.com-personal
    HostName github.com
    UseKeychain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github_ed25519_personal
  
Host gitlab.com-personal
    HostName gitlab.com
    UseKeychain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/gitlab_ed25519_personal

